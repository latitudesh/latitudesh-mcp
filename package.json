{
  "name": "latitudesh",
  "version": "0.0.2",
  "description": "Model Context Protocol (MCP) Server for the Latitude.sh API",
  "author": "Latitude.sh <support@latitude.sh>",
  "license": "MIT",
  "keywords": ["mcp", "latitude", "latitudesh", "model-context-protocol", "ai", "llm"],
  "repository": {
    "type": "git",
    "url": "https://github.com/latitudesh/latitudesh-mcp.git"
  },
