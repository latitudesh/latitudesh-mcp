  "devDependencies": {
    "@anthropic-ai/dxt": "^0.1.0",
    "@eslint/js": "^9.26.0",
    "@types/bun": "^1.2.13",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.20",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.1"
